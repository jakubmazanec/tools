{
  "name": "@jakubmazanec/ui",
  "version": "0.1.5",
  "description": "React component library.",
  "repository": {
    "type": "git",
    "url": "git@github.com:jakubmazanec/tools.git",
    "directory": "packages/ui"
  },
  "license": "LGPL-3.0-only",
  "author": "Jakub Mazanec <jakub@mazanec.dev>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./build/main.js",
    "./package.json": "./package.json",
    "./development": "./build/development.js",
    "./styles": "./build/styles.css"
  },
  "files": [
    "build",
    "source"
  ],
  "scripts": {
    "build": "del-cli build tsconfig.tsbuildinfo && esbuild \"source/**/*\" --platform=node --target=node22.11 --outdir=build --sourcemap --loader:.woff2=copy && tsc && storybook build",
    "clean": "del-cli coverage build *.tsbuildinfo",
    "develop": "concurrently \"esbuild \"source/**/*\" --platform=node --target=node22.11 --outdir=build --sourcemap --watch\" \"tsc --watch\" \"storybook dev -p 6006 --no-open\"",
    "document": "del-cli docs && typedoc --options typedoc.config.cjs",
    "format": "prettier . --write --ignore-unknown",
    "lint": "eslint .",
    "print-colors": "tsx tools/print-colors.ts",
    "test": "del-cli coverage && vitest run --coverage --passWithNoTests",
    "typecheck": "tsc --project tsconfig.typecheck.json"
  },
  "dependencies": {
    "@conform-to/react": "^1.1.5",
    "@conform-to/zod": "^1.1.5",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@jakubmazanec/ts-utils": "^3.0.1",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-table": "^8.20.5",
    "culori": "^4.0.1",
    "lodash": "^4.17.21",
    "tailwind-merge": "^2.5.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@jakubmazanec/eslint-config": "^4.1.0",
    "@storybook/addon-controls": "^8.4.4",
    "@storybook/addon-essentials": "^8.4.4",
    "@storybook/addon-interactions": "^8.4.4",
    "@storybook/addon-links": "^8.4.4",
    "@storybook/blocks": "^8.4.4",
    "@storybook/preview-api": "^8.4.4",
    "@storybook/react": "^8.4.4",
    "@storybook/react-vite": "^8.4.4",
    "@storybook/test": "^8.4.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/culori": "^2.1.1",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitest/coverage-v8": "^2.1.5",
    "autoprefixer": "^10.4.20",
    "chromatic": "^11.18.1",
    "concurrently": "^9.1.2",
    "del-cli": "^6.0.0",
    "esbuild": "^0.24.2",
    "eslint": "^9.17.0",
    "happy-dom": "^16.2.8",
    "prettier": "^3.4.2",
    "prettier-plugin-packagejson": "^2.5.6",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.4.4",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typedoc": "^0.27.6",
    "typedoc-plugin-markdown": "^4.3.3",
    "typescript": "^5.7.2",
    "vitest": "^2.1.5"
  },
  "peerDependencies": {
    "react": "18.x",
    "react-dom": "18.x",
    "tailwindcss": "3.x"
  },
  "packageManager": "npm@10.9.0",
  "engines": {
    "node": "^22.11.0"
  }
}
