---
to: '.editorconfig'
strategy: overwrite
---
#
# DO NOT EDIT!
# This file was autogenerated by Carson.
# Changes may cause incorrect behavior and will be lost when the file is regenerated.
#
# Run `npx carson update workspace` to regenerate.
#

root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[**.{js,jsx,mjs,cjs,ts,tsx,cts,mts,css,graphql,json,yaml,yml}]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[**.{md,mdx}]
indent_style = space
indent_size = 2
trim_trailing_whitespace = false
