{
  "name": "@jakubmazanec/eslint-config",
  "version": "4.1.8",
  "description": "ESLint config for TypeScript projects.",
  "repository": {
    "type": "git",
    "url": "git@github.com:jakubmazanec/tools.git",
    "directory": "packages/eslint-config"
  },
  "license": "MIT",
  "author": "Jakub Mazanec <jakub@mazanec.dev>",
  "sideEffects": false,
  "type": "module",
  "exports": "./build/main.js",
  "files": [
    "build",
    "source"
  ],
  "scripts": {
    "build": "del-cli build tsconfig.tsbuildinfo && esbuild \"source/**/*\" --platform=node --target=node24.3 --outdir=build --sourcemap && tsc",
    "check-rules": "tsx tools/check-rules/main.ts",
    "clean": "del-cli coverage build *.tsbuildinfo",
    "format": "prettier . --write --ignore-unknown",
    "lint": "eslint .",
    "typecheck": "tsc --project tsconfig.typecheck.json"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "confusing-browser-globals": "^1.0.11",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.0.0-beta-ebf51a3-20250411",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-testing-library": "^7.6.6",
    "eslint-plugin-unicorn": "^60.0.0",
    "globals": "^16.5.0"
  },
  "devDependencies": {
    "@types/confusing-browser-globals": "^1.0.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^24.10.1",
    "del-cli": "^6.0.0",
    "esbuild": "^0.27.0",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "packageManager": "npm@11.3.0",
  "engines": {
    "node": "^24.11.1",
    "npm": "^11.3.0"
  }
}
