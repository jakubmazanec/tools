{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build", "codegen"],
      "outputs": ["build/**"]
    },
    "clean": {
      "cache": false
    },
    "codegen": {
      "cache": false
    },
    "develop": {
      "dependsOn": ["^build", "codegen"],
      "cache": false,
      "persistent": true
    },
    "document": {
      "dependsOn": ["^build", "codegen"],
      "outputs": ["docs/**"]
    },
    "format": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["@jakubmazanec/eslint-config#build", "^build", "codegen"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build", "codegen", "typecheck", "lint"],
      "outputs": ["coverage/**"]
    },
    "typecheck": {
      "dependsOn": ["^build", "codegen"],
      "outputs": []
    }
  },
  "globalEnv": ["CI"]
}
